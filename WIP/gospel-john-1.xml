<TEI xmlns="http://www.tei-c.org/ns/1.0">
   <teiHeader>
      <fileDesc>
         <titleStmt>
            <title>Russian Gospel</title>
         </titleStmt>
         <publicationStmt>
            <p>Unpublished</p>
         </publicationStmt>
         <sourceDesc>
            <p>???</p>
         </sourceDesc>
      </fileDesc>
